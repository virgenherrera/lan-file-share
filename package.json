  "keywords": [],
  "preferGlobal": true,
  "bin": {
    "lfs": "./bin/cli"
  },
  "homepage": "https://github.com/virgenherrera/lan-file-share-api#readme",
  "bugs": {
    "url": "https://github.com/virgenherrera/lan-file-share-api/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/virgenherrera/lan-file-share-api.git"
  },
  "contributors": [
    "Hugo Virgen (https://github.com/virgenherrera)"
  ],
  "engines": {
    "node": ">=18.12.0 <19",
    "npm": ">=8.6.0"
  },
  "scripts": {
    "test": "run-s -l cleanup test:* build:*",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "UT:develop": "jest --watch --maxWorkers=1",
    "UT:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "e2e:develop": "jest --config jest-e2e.config.ts --watch --maxWorkers=1",
    "e2e:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "build:api-docs": "ts-node -r tsconfig-paths/register src/openApi.ts",
    "build:api-docs-ui": "npx redoc-cli build -o api-docs/index.html api-docs/swagger.json",
    "build:app": "nest build",
    "test:static": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test:unit": "jest",
    "test:e2e": "jest --config jest-e2e.config.ts",
    "cleanup": "rimraf api-docs/ coverage/ dist/",
    "install": "run-s cleanup build:*"
  },
  "dependencies": {
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^6.3.0",
    "adm-zip": "^0.5.10",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "date-fns": "^2.29.3",
    "helmet": "^6.1.5",
    "lan-file-share-web-ui": "github:virgenherrera/lan-file-share-angular-web-ui#1.2.0",
    "nest-winston": "^1.9.1",
    "qrcode-terminal": "^0.12.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.0",
    "rxjs": "^7.8.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.4.0",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@types/adm-zip": "^0.5.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.11",
    "@types/rimraf": "^3.0.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint": "^8.38.0",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  }
