{
  "name": "nest-base",
  "version": "0.0.1",
  "description": "",
  "license": "MIT",
  "keywords": [],
  "author": "",
  "private": true,
  "engines": {
    "node": "^16.14.0",
    "npm": "^8.3.1"
  },
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test:develop": "jest --coverage --watch --maxWorkers=1 --verbose false",
    "local:ci": "run-s -l check:format test:cov build:api-docs build",
    "build:api-docs": "ts-node -r tsconfig-paths/register src/core/builders/open-api.builder.ts",
    "build": "nest build",
    "cleanup": "rimraf dist/ reports/",
    "check:format": "run-s check:prettier check:eslint",
    "check:eslint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "check:prettier": "prettier --check \"src/**/*.ts\"",
    "fix:format": "run-s -l fix:prettier fix:eslint",
    "fix:eslint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "fix:prettier": "prettier --write \"src/**/*.ts\"",
    "prebuild": "npm run cleanup",
    "prestart:prod": "npm run build",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test": "jest"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.6",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/platform-express": "^8.2.6",
    "@nestjs/swagger": "^5.2.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "date-fns": "^2.28.0",
    "helmet": "^4.6.0",
    "nest-winston": "^1.6.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "winston": "^3.5.1",
    "winston-daily-rotate-file": "^4.6.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.0",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.6",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.21",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^27.4.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.4.4"
  }
}
